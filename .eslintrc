{
	"extends": [
		"eslint:recommended", "plugin:react/recommended"
	],
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true
	},
	"plugins": [
		"react"
	],
	"rules": {
		"no-console": 0,
		"no-underscore-dangle": 0,
		"quotes": [2, "single"],
		"react/prop-types": 0,
		"react/display-name": 0
	}

}